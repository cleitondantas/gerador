{
  "manifest_version": 3,
  "name": "Gerador de Senhas",
  "version": "1.0",
  "description": "Gerador de senhas personalizado baseado em seed, versão e serviço",
  "permissions": ["activeTab", "tabs"],
  "action": {
    "default_popup": "sidebar.html",
    "default_icon": {
